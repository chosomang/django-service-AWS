[
  {
    "selector": "core",
    "style": {
      "selection-box-color": "#AAD8FF",
      "selection-box-border-color": "#8BB0D0",
      "selection-box-opacity": "0.5"
    }
  },
  {
    "selector": "node",
    "style": {
      "width": "mapData(score, 400, 800, 100, 200)",
      "height": "mapData(score, 400, 800, 100, 200)",
      "content": "data(name)",
      "font-size": "14px",
      "text-valign": "center",
      "text-halign": "center",
      "border-color": "#555",
      "background-color": "#555",
      "text-outline-color": "#555",
      "text-outline-width": "0.5px",
      "color": "#fff",
      "overlay-padding": "6px",
      "z-index": "10"
    }
  },
  {
    "selector": "node[label=\"Rule\"]",
    "style": {
      "background-color": "#c22111"
    }
  },
  {
    "selector": "node[label=\"Log\"]",
    "style": {
      "background-color": "#1cc88a"
    }
  },
  {
    "selector": "node[label=\"Account\"]",
    "style": {
      "background-color": "#f6c23e",
      "font-size": "30px"
    }
  },
  {
    "selector": "node[label=\"Between\"]",
    "style": {
      "background-color": "#d1c08c"
    }
  },
  {
    "selector": "node[label=\"Date\"]",
    "style": {
      "background-color": "#4e73df"
    }
  },
  {
    "selector": "node[label=\"Flow\"]",
    "style": {
      "background-color": "#913781"
    }
  },
  {
    "selector": "node[label=\"Role\"]",
    "style": {
      "background-color": "#f27b44"
    }
  },
  {
    "selector": "node[?attr]",
    "style": {
      "shape": "rectangle",
      "background-color": "#aaa",
      "text-outline-color": "#aaa",
      "width": "16px",
      "height": "16px",
      "font-size": "6px",
      "z-index": "1"
    }
  },
  {
    "selector": "node[?query]",
    "style": {
      "background-clip": "none",
      "background-fit": "contain"
    }
  },
  {
    "selector": "node:selected",
    "style": {
      "border-width": "6px",
      "border-color": "#AAD8FF",
      "border-opacity": "0.5",
      "background-color": "#4e73df",
      "text-outline-color": "#77828C"
    }
  },
  {
    "selector": "edge",
    "style": {
      "curve-style": "bezier",
      "haystack-radius": "1",
      "opacity": "0.4",
      "line-color": "#ccc",
      "width": "mapData(weight, 0, 1, 1, 5)",
      "overlay-padding": "3px",
      "label": "data(name)",
      "font-size": "14px",
      "target-arrow-color": "#ccc",
      "target-arrow-shape": "vee"
    }
  },
  {
    "selector": "node.unhighlighted",
    "style": {
      "opacity": "0.2"
    }
  },
  {
    "selector": "edge.unhighlighted",
    "style": {
      "opacity": "0.05"
    }
  },
  {
    "selector": ".highlighted",
    "style": {
      "z-index": "1000"
    }
  },
  {
    "selector": "node.highlighted",
    "style": {
      "border-width": "6px",
      "border-color": "#AAD8FF",
      "border-opacity": "0.5",
      "background-color": "#394855",
      "text-outline-color": "#394855"
    }
  },
  {
    "selector": "edge.filtered",
    "style": {
      "opacity": "0"
    }
  },
  {
    "selector": "edge[group=\"coexp\"]",
    "style": {
      "line-color": "#d0b7d5"
    }
  },
  {
    "selector": "edge[group=\"coloc\"]",
    "style": {
      "line-color": "#a0b3dc"
    }
  },
  {
    "selector": "edge[group=\"gi\"]",
    "style": {
      "line-color": "#90e190"
    }
  },
  {
    "selector": "edge[group=\"path\"]",
    "style": {
      "line-color": "#9bd8de"
    }
  },
  {
    "selector": "edge[group=\"pi\"]",
    "style": {
      "line-color": "#eaa2a2"
    }
  },
  {
    "selector": "edge[group=\"predict\"]",
    "style": {
      "line-color": "#f6c384"
    }
  },
  {
    "selector": "edge[group=\"spd\"]",
    "style": {
      "line-color": "#dad4a2"
    }
  },
  {
    "selector": "edge[group=\"spd_attr\"]",
    "style": {
      "line-color": "#D0D0D0"
    }
  },
  {
    "selector": "edge[group=\"reg\"]",
    "style": {
      "line-color": "#D0D0D0"
    }
  },
  {
    "selector": "edge[group=\"reg_attr\"]",
    "style": {
      "line-color": "#D0D0D0"
    }
  },
  {
    "selector": "edge[group=\"user\"]",
    "style": {
      "line-color": "#f0ec86"
    }
  }
]